@model Cafe.Menu.Entity.Property

<form id="saveForm" method="post" action="/Property/Save">
    @Html.HiddenFor(v=> v.PropertyId)

    <div class="form-group">
        <div class="row">
            <div class="col-lg-3">
                <label for="exampleFormControlInput1">Özellik Adı</label>
            </div>
            <div class="col-lg-9">
                @Html.TextBoxFor(m => m.Key, new { @class = "form-control", @required = "required" })
            </div>
        </div>
    </div>

    <div class="form-group">
        <div class="row">
            <div class="col-lg-3">
                <label for="exampleFormControlInput1">Değer</label>
            </div>
            <div class="col-lg-9">
                @Html.TextBoxFor(m => m.Value, new { @class = "form-control", @required = "required" })
            </div>
        </div>
    </div>

    <div class="form-group">
        <div class="row">
            <div class="col-lg-12">
                <input type="submit" value="Kaydet" />
            </div>
        </div>
    </div>
</form>